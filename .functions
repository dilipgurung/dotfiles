function set_prefix {
    BRANCH=`__git_ps1`
    if [[ -z $BRANCH ]]; then
        echo "${NORMAL}☃ π"
    else
        echo "${UNDERLINE}+"
    fi
}

# Laravel
artisan() {
    if [ -f bin/artisan ]; then
        php bin/artisan "$@"
    else
        php artisan "$@"
    fi
}

alias serve='artisan serve'
alias tinker='artisan tinker'

# Misc PHP
t() {
    if [ -f vendor/bin/phpunit ]; then
        vendor/bin/phpunit "$@"
    else
        phpunit "$@"
    fi
}