# Laravel
alias a='php artisan'

# PHPUnit
alias t='clear && ./vendor/bin/phpunit'

# Elastic Search
alias es='elasticsearch --config=/usr/local/opt/elasticsearch17/config/elasticsearch.yml'

# Docker
alias dps="docker ps"
alias drs="docker images | grep \"<none>\" | awk '{print $3}' | xargs docker rmi"
alias dc="docker-compose"
alias di="docker images"

# Git Aliases
alias gc="git commit -am"
alias gs="git status"
alias gp="git pull"
alias ga="git add --all"
alias gr="git reset --hard HEAD"

# Flush DNS cache
alias flushdns='dscacheutil -flushcache;killall -HUP mDNSResponder'

# Vagrant
alias vm="vagrant ssh"
alias vu="vagrant up"
alias vh="vagrant halt"
alias vp="vagrant provision"
alias vs="vagrant status"

# Sublime Text Editor
alias sublime="/Applications/Sublime\ Text.app/Contents/SharedSupport/bin/subl"

# Start kubernetes UI
alias kube="kubectl proxy --port=8181"

# list linux style
alias 'ls=ls -GF'
alias 'll=ls -aliGF'
