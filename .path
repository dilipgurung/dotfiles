# Golang
export GOPATH=$HOME/Go
export GOROOT=/usr/local/opt/go/libexec

# Composer
COMPOSER_PATH=~/.composer/vendor/bin

# Packer
PACKER=/usr/local/packer

# Terraform
TERRAFORM=/usr/local/terraform/bin

# Update PATH
export PATH=/user/local/bin:/usr/local/sbin:$PATH:$GOPATH/bin:$GOROOT/bin:$PACKER:$COMPOSER_PATH:$TERRAFORM
